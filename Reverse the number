#include <stdio.h>

int main(void) 
{	 int N,T,i,sum,r;
     scanf("%d",&T);
     while(T--)
     {
         scanf("%d",&N);
         while(N>0)
         {
             r=N%10;
             sum=sum*10+r;
             N=N/10;
         }
         printf("%d\n",sum);
         sum=0;
     }
   
    
	return 0;
}
