#include <stdio.h>

int main(void) {
    int T,N,i,a=0;
    scanf("%d",&T);
    while(T--)
    {
        scanf("%d",&N);
        for(i=1;i<=N;i++)
        {
            if(N%i==0)
            a++;
        }
        if(a==2)
        printf("yes\n");
        else
        printf("no\n");
        a=0;
    }
	// your code goes here
	return 0;
}
