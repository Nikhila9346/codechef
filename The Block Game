#include <stdio.h>

int main(void) {
    int T,N,r,a,sum=0;
    scanf("%d",&T);
    while(T--)
    {
     scanf("%d",&N);
     a=N;
     while(N>0)
     {
         r=N%10;
         sum=sum*10+r;
         N=N/10;
        
     }
        if(a==sum)
        printf("wins\n");
        else
        printf("loses\n");
        sum=0;
    }
	// your code goes here
	return 0;
}
